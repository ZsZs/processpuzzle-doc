{
  "name": "processpuzzle-doc",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --vendorSourceMap",
    "build": "ng build",
    "build-dev": "npm run build-lib-dev && npm run build-app-dev",
    "build-prod": "npm run build-lib-prod && npm run build-app-prod",
    "build-app-dev": "ng build processpuzzle-doc-app --configuration=development",
    "build-app-prod": "ng build processpuzzle-doc-app --prod --configuration=production",
    "build-lib-dev": "ng build processpuzzle-doc-lib --configuration=development",
    "build-lib-prod": "ng build processpuzzle-doc-lib --prod --configuration=production",
    "bump-lib-patch-version": "cd projects/processpuzzle-doc-lib && npm version patch && cd ../../",
    "bump-patch-version": "npm version patch && npm run bump-lib-patch-version && git add . && git commit -m \"commit library version bump\" && git push && git push --tags",
    "bump-lib-minor-version": "cd projects/processpuzzle-doc-lib && npm version minor && cd ../../",
    "bump-minor-version": "npm version minor && npm run bump-lib-minor-version && git add . && git commit -m \"commit library version bump\" && git push && git push --tags",
    "ci": "npm run lint && npm run test && npm run build-prod && npm run npm-pack",
    "deploy": "firebase deploy",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npm-pack": "cd dist/processpuzzle-doc-lib && npm pack",
    "test": "npm run test-lib",
    "test-app": "ng test processpuzzle-doc-app --browsers ChromeHeadless --code-coverage",
    "test-app-watch": "ng test processpuzzle-doc-app --browsers Chrome --watch",
    "test-lib": "ng test processpuzzle-doc-lib --browsers ChromeHeadless --configuration=unit-test --code-coverage",
    "test-lib-watch": "ng test processpuzzle-doc-lib --browsers Chrome --watch --configuration=unit-test",
    "package": "npm run build-dev && npm run npm-pack",
    "publish": "cd dist/processpuzzle-doc-lib && npm publish",
    "start-c9": "ng serve --host 0.0.0.0 --port 8080 --disableHostCheck"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "ContentTools": "^1.6.10",
    "angularfire2": "^5.1.0",
    "core-js": "^2.5.7",
    "firebase": "^5.5.6",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "json2typescript": "^1.0.6",
    "materialize-css": "^1.0.0",
    "ngx-logger": "^3.3.6",
    "ngx-materialize": "^6.1.3",
    "processpuzzle-util": "^0.3.2",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular-devkit/build-ng-packagr": "~0.10.3",
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": ">=0.33.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^3.1.5"
  }
}
